  "license": "LGPL-3.0",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomAFrench/NoteStream.git"
  },
  "scripts": {
    "prepare": "yarn clean && yarn lint --fix",
